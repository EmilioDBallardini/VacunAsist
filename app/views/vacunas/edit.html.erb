<%= form_for @vacuna do |f| %>

    <div>
        <%= f.label "Vacuna" %>
        <%= f.select :tipo_vacuna_id, options_from_collection_for_select(TipoVacuna.all, :id, :nombre) %>
    </div>
    <div>
        <%= f.label :lote %>
        <%= f.text_field :lote %>
    </div>
    <div>
        <%= f.label :cantidad %>
        <%= f.number_field :cantidad %>
    </div>
    <div>
        <%= f.label :vaccination_id %>
        <%= f.select :vaccination_id, options_from_collection_for_select(Vaccination.all, :id, :nombre) %>
    </div>
    <div>
        <%= f.submit %>
    </div>
<%end%>
